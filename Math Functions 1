import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        float f=sc.nextFloat();
        int sq=sc.nextInt();
        int bs=sc.nextInt();
        int po=sc.nextInt();
        System.out.println(Math.round(Math.floor(f)));
        System.out.println(Math.round(Math.ceil(f)));
        System.out.println(Math.round(Math.sqrt(sq)));
        System.out.println(Math.round(Math.pow(bs,po)));
        }
}
