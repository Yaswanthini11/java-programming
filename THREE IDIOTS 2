import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
        int d=sc.nextInt();
        float x = (float)(a + c)/2;
        float y = (float)(b + d)/2;
        System.out.println("Binoy's house is located at ("+x+","+y+")");



        
   }
}
