import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
       String st=sc.nextLine();
       char arr[]=st.toCharArray();
       int n=arr.length;

        for(int i=n-1;i>=0;i--){
        System.out.print(arr[i]);
     }
      
      


  }
}
